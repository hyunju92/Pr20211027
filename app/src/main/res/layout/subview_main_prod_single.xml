<?xml version="1.0" encoding="utf-8"?>
<layout xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:app="http://schemas.android.com/apk/res-auto">

    <data>

        <variable
            name="mainViewModel"
            type="hyunju.com.pr20211027.main.vm.MainViewModel" />

        <variable
            name="sharedViewModel"
            type="hyunju.com.pr20211027.home.vm.HomeViewModel" />


        <variable
            name="data"
            type="hyunju.com.pr20211027.main.view.data.MainProdSingleItem" />

        <import
            alias="ExtensionUtil"
            type="hyunju.com.pr20211027.util.BindingAdapterUtilKt" />

    </data>

    <androidx.constraintlayout.widget.ConstraintLayout
        android:id="@+id/subviewMainProdSingleCl"
        android:layout_width="match_parent"
        android:layout_height="wrap_content"
        android:layout_margin="4dp">

        <include
            android:id="@+id/subviewMainProdSingleItem"
            layout="@layout/item_common_card"
            android:layout_width="match_parent"
            android:layout_height="wrap_content"
            app:cardCount="@{1}"
            app:data="@{data.prod}"
            app:layout_constraintStart_toStartOf="parent" />
    </androidx.constraintlayout.widget.ConstraintLayout>
</layout>